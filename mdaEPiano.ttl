@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.
@prefix pg: <http://ll-plugins.nongnu.org/lv2/ext/portgroups#>.
@prefix ev: <http://lv2plug.in/ns/ext/event#>.
@prefix ui: <http://lv2plug.in/ns/extensions/ui#>.

<http://rekado.wurmus.de/lv2/mdaEPiano/gui>
  a ui:GtkUI;
  ui:binary <mdaEPianoGUI.so>;
  ui:requiredFeature ui:makeResident;
  ui:optionalFeature ui:Event.

<http://rekado.wurmus.de/lv2/mdaEPiano/out> a pg:StereoGroup.

<http://rekado.wurmus.de/lv2/mdaEPiano>
	a lv2:Plugin, lv2:InstrumentPlugin;
	lv2:binary <mdaEPiano.so>;
	doap:name "mdaEPiano";
	doap:maintainer [
		a foaf:Person;
		foaf:name "Ricardo Wurmus";
		foaf:homepage "http://rekado.wurmus.de"
	];
	doap:license <http://usefulinc.com/doap/licenses/gpl>;
	ll:pegName "p";
	ui:ui <http://rekado.wurmus.de/lv2/mdaEPiano/gui>;

	lv2:port [
		a ev:EventPort, lv2:InputPort;
		lv2:index 0;
		ev:supportsEvent <http://lv2plug.in/ns/ext/midi#MidiEvent>;
		lv2:symbol "midi";
		lv2:name "MIDI";
	],

	[
		a lv2:AudioPort, lv2:OutputPort;
		lv2:index 1;
		lv2:symbol "left";
		lv2:name "Left";
		pg:membership [
			pg:group <http://rekado.wurmus.de/lv2/mdaEPiano/out>;
			pg:role pg:leftChannel;
		];
	],

	[
		a lv2:AudioPort, lv2:OutputPort;
		lv2:index 2;
		lv2:symbol "right";
		lv2:name "Right";
		pg:membership [
			pg:group <http://rekado.wurmus.de/lv2/mdaEPiano/out>;
			pg:role pg:rightChannel;
		];
	].
